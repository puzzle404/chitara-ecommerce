<h1>Orders#index</h1>
<p>Find me in app/views/orders/index.html.erb</p>
<h2>All Orders</h2>

<% @orders.each do |order| %>
  <p>Name: <%= order.comment%></p>
  <p>Email: <%= order.user_id%></p>
  <p>order Items: </p>
  <ul>
    <% order.order_items.each do |item| %>
    <li><%= item.quantity %> x <%= item.sandal.model %></li>
    <% end %>
  </ul>
<% end %>
